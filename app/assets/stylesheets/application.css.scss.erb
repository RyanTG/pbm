/** variables **/

$sans: 'Nunito', Helvetica, Arial, sans-serif;
$serif: 'Lato', "Lucida Grande", Tahoma, sans-serif;
$red: #fd0091;
$lightpink: #fff1f1;
$blue: #e0f1ff;
$darkblue: #232347;
$purple: #e0e0ff;
$grey: #7f7fa0;
$lightgrey: #fafaff;
$medgrey: #aeb1d2;
$darkgrey: #47475f;
$black: #000e18;
$bold: 700;

/** mixins **/

@mixin radius ($radii...) {
  border-radius: $radii;
}

@mixin padding ($pads...) {
  padding: $pads;
}

@mixin set ($width, $height, $padding, $margin) {
  width: $width;
  height: $height;
  padding: $padding;
  margin: $margin;
}

/** Core **/

* {
  outline: none;
}

*,
*:before,
*:after {
  box-sizing: inherit;
}

.clear {
  clear: both;
}

button::-moz-focus-inner {
  padding: 0 0 2px 0 !important;
  border: 0 none !important;
}

html {
  min-height: 100%;
  position: relative;
  box-sizing: border-box;
  background: #FFFFFF;
}

body {
  margin: 0 auto;
  padding: 0px;
  background-color: #FFFFFF;
  background-size: 4px auto;
  color: $black;
  font-family: $sans;
  -webkit-font-smoothing: antialiased;
}

