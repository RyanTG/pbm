/** Map/intro **/

#intro p {
  margin: 10px 0;
  padding: 5px;
}

@-webkit-keyframes colorChange {
  0% {
    color: #2e2322;
  }

  50% {
    color: $darkgrey;
    text-shadow: 1px 1px 5px #CCCCCC;
    background-color: $blue;
  }

  100% {
    color: #2e2322;
  }
}

@-moz-keyframes colorChange {
  0% {
    color: #2e2322;
  }

  50% {
    color: $darkgrey;
    text-shadow: 1px 1px 5px #CCCCCC;
    background-color: $blue;
  }

  100% {
    color: #2e2322;
  }
}

@keyframes colorChange {
  0% {
    color: #2e2322;
  }

  50% {
    color: $darkgrey;
    text-shadow: 1px 1px 5px #CCCCCC;
    background-color: $blue;
  }

  100% {
    color: #2e2322;
  }
}

@-o-keyframes colorChange {
  0% {
    color: #2e2322;
  }

  50% {
    color: $darkgrey;
    text-shadow: 1px 1px 5px #CCCCCC;
    background-color: $blue;
  }

  100% {
    color: #2e2322;
  }
}

span.begin {
  padding: 10px;
  display: block;
  background-color: $lightgrey;

  -webkit-animation-duration: 5s;
  -webkit-animation-name: colorChange;
  -webkit-animation-delay: 1s;
  -webkit-animation-timing-function: ease-in-out;
  -webkit-animation-iteration-count: 1;

  -moz-animation-duration: 5s;
  -moz-animation-name: colorChange;
  -moz-animation-delay: 1s;
  -moz-animation-timing-function: ease-in-out;
  -moz-animation-iteration-count: 1;

  -o-animation-duration: 5s;
  -o-animation-name: colorChange;
  -o-animation-delay: 1s;
  -o-animation-timing-function: ease-in-out;
  -o-animation-iteration-count: 1;

  animation-duration: 5s;
  animation-name: colorChange;
  animation-delay: 1s;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}

#map,
#map_canvas {
  background: #ffffff;
}

#region_map {
  width: 700px;
  height: 450px;
  margin: 10px 0;
  border: 5px solid $blue;
  box-shadow: 1px 2px 7px rgba(90, 100, 117, 0.3);
}

#map {
  left: 42%;
  width: 100%;
  overflow: hidden;
  position: fixed;
  height: calc(100% - 70px) !important;
  z-index: -1;
}

#map_canvas {
  height: 100% !important;
  margin-left: -42%;
}

.mask {
  background-color: rgba(245, 251, 255, 0.7);
  position: absolute;
  z-index: 9999;
  height: 100%;
  width: 100%;
}

.closest_button {
  border: 3px solid $darkgrey;
  background: $blue;
  @include radius(10px);
  font-size: 22px;
  text-align: center;
  top: 200px;
  right: 0;
  left: 0;
  height: 100px;
  width: 365px;
  margin: auto;
  padding: 5px;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 150;
  line-height: 42px;
  box-shadow: 5px 7px 10px rgba(90, 100, 117, 0.5);
}

.closest_button a {
  text-decoration: none;
  color: $darkgrey;
}

.closest_button a:hover {
  text-decoration: none;
  color: $red;
}

#intro {
  padding: 5px 20px;
}

#not_found {
